C
C LOGICAL UNIT NUMBERS
C
      INCLUDE  'plun'
C
C MACHINE DEPENDENT PARAMETERS
C
      INCLUDE  'imachine'
C
C STANDARD PARAMETERS
C
      INTEGER MZP1,MEQUAT,MX,MIXS,MVESM,MDEBWR,MDEBPT,MTVEC
     &      , MACRO,MACRO1,MACRO2,MACRO3,MVESC,MXTRAN,MXJET,MPROF
      PARAMETER(MZP1=MZ+1,MEQUAT=5+MZ*2+MZS,MX=35)
      PARAMETER( MIXS=10 , MVESM=1000 , MVESC=10 )
      PARAMETER( MDEBWR = 30 , MDEBPT = 200 )
      PARAMETER( MACRO=30,MACRO1=MACRO+1,MACRO2=2+MACRO+1,MACRO3=2 )
      PARAMETER( MXTRAN=10, MXJET=MXTRAN , MPROF=10 , MTVEC=20 )
C
      REAL*8 STIMIN
      PARAMETER( STIMIN = 10.0D+00 )
