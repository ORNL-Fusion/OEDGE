c     -*-Fortran-*-
c
c     dimension variables and arrays for multi-charge model rates
c
c     labelrt - header information from 'b2frates' data file
c     rtnt    - number of intervals in 'b2frates' temperature data
c     rtnn    - number of intervals in 'b2frates' density data   
c     rtns    - number of species in a 'b2frates' file           
c     rtnsd   - total number of species from all 'b2frates' files
c     rtza    - atomic charge state
c     rtzn    - nuclear charge state
c     rtza2   - atomic charge state squared
c     rtt     - [eV] temperature data in 'b2frates' table
c     rtn     - [/m**3] density data in 'b2frates' table
c     rtlt    - ln(rtt) where rtt[eV] is 'b2frates' temperature data
c     rtln    - ln(rtn) where rtn[/m**3] is 'b2frates' density data
c     rtlsa   - ln(rtsa) where rtsa[m**3/s] is 'b2frates' 
c               data for ionization
c     rtlra   - ln(rtra) where rtra[m**3/s] is 'b2frates' 
c               data for recombination
c     rtlqa   - ln(rtqa) where rtqa[eV*m**3/s] is 'b2frates' 
c               data for electron energy loss
c     rtlcx   - ln(rtcx) where rtcx[m**3/s] is 'b2frates' 
c               data for c-x on neutral hydrogen
c
      integer   maxrtnsd,maxrtnt,maxrtnn
      parameter (maxrtnsd=maxizs,maxrtnt=50,maxrtnn=20)
c
      common /adpak/ rtnt,rtnn,rtns,rtnsd,rtza,rtzn,rtza2,rtt,rtn,
     >               rtlt,rtln,rtlsa,rtlra,rtlqa,rtlcx,
     >               mcfile,labelrt
      save /adpak/
c
      character*120  labelrt(1:12)
      character*120  mcfile
c
      integer	rtnt,rtnn,rtns,rtnsd
      real      rtza(0:maxrtnsd-1),rtzn(0:maxrtnsd-1),
     >          rtza2(0:maxrtnsd-1)
      real      rtt(0:maxrtnt),rtn(0:maxrtnn),
     >          rtlt(0:maxrtnt),rtln(0:maxrtnn)
      real      rtlsa(0:maxrtnt,0:maxrtnn,0:maxrtnsd-1),
     >          rtlra(0:maxrtnt,0:maxrtnn,0:maxrtnsd-1),
     >          rtlqa(0:maxrtnt,0:maxrtnn,0:maxrtnsd-1),
     >          rtlcx(0:maxrtnt,0:maxrtnn,0:maxrtnsd-1)
