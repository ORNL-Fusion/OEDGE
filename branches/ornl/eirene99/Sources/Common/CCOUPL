*COMDECK CCOUPL
      PARAMETER (NCOUPL=4*NPLS+8,
     P           MCOUPL=1*NPLS+9*NSTEP*NGITT+NSTEP+17+2*NAIN+2*NLIMPS,
     P           LCOUPL=6+NSTRA)
      COMMON /CCOUPL/
     R  FCTE(NPLS),D(NPLS),FL(NPLS),BMASS(NPLS),
     R  CHGP,CHGEE,CHGEI,CHGMOM,
     R  B2BREM,B2RAD,B2QIE,B2VDP,
     I  NDT(NSTEP,NGITT),NINCT(NSTEP,NGITT),NIXY(NSTEP,NGITT),
     I  NTIN(NSTEP,NGITT),NTEN(NSTEP,NGITT),NIFLG(NSTEP,NGITT),
     I  NPTC(NSTEP,NGITT),NSPZI(NSTEP,NGITT),NSPZE(NSTEP,NGITT),
     I  NTGPRT(NSTEP),
     I  NTARGI,NSTRI,NFLA,NCUTB,NCUTL,IFLB(NPLS),NDXA,NDYA,
     I  NCLMI,NBLCKI,NPRNVI,NPRTVI,NPRDVI,
     I  NMODEI,NFILNN,NCUTB_SAVE,
     I  NAINB,NAINS(NAIN),NAINT(NAIN),
     I  NAOTB,NAOTS(NLIMPS),NAOTT(NLIMPS),
     L  LBALAN,LSYMET,LPRSOU,
     L  LNLPLG,LNLDRF,LTRCFL,LNLVOL(NSTRA)
      LOGICAL LBALAN,LSYMET,LPRSOU
      LOGICAL LNLPLG,LNLDRF,LTRCFL,LNLVOL
