c slmod end
C  FROM HERE: NO EQUIVALENCE
     I NFIRST(NTALV),NADDV(NTALV),NFRSTW(NTALS),NADDW(NTALS),
     I IRESC1(NTALV),IRESC2(NTALV)
C
       DIMENSION
     R PDENA(NATM,NRAD),PDENM(NMOL,NRAD),PDENI(NION,NRAD),
     R EDENA(NATM,NRAD),EDENM(NMOL,NRAD),EDENI(NION,NRAD),
     R PAEL(NRAD),PAAT(NATM,NRAD),PAML(NMOL,NRAD),PAIO(NION,NRAD),
     P PAPL(NPLS,NRAD),
