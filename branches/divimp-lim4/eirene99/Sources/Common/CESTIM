*COMDECK CESTIM
      PARAMETER (NESTM1=NVOLTL*NRAD,
     P           NESTM2=NSRFTL*NLMPGS,
C
     P           NESTIM=NESTM1+NESTM2)
      COMMON /CESTIM/
C  NESTM1, REAL, VOLUME AVERAGED TALLIES
C
C
c slmod begin - f90
     R PDENA,PDENM,PDENI,EDENA,EDENM,EDENI,PAEL,PAAT,PAML,PAIO,PAPL,
     R PMEL,PMAT,PMML,PMIO,PMPL,PIEL,PIAT,PIML,PIIO,PIPL,EAEL,EAAT,
     R EAML,EAIO,EAPL,EMEL,EMAT,EMML,EMIO,EMPL,EIEL,EIAT,EIML,EIIO,
     R EIPL,ADDV,COLV,SNAPV,COPV,BGKV,ALGV,PGENA,PGENM,PGENI,EGENA,
     R EGENM,EGENI,VGENA,VGENM,VGENI,
C  NESTM2, REAL, SURFACE AVERAGED TALLIES
     R POTAT (NATM,NLMPGS),PRFAAT(NATM,NLMPGS),PRFMAT(NATM,NLMPGS),
     R PRFIAT(NATM,NLMPGS),PRFPAT(NATM,NLMPGS),POTML (NMOL,NLMPGS),
     R PRFAML(NMOL,NLMPGS),PRFMML(NMOL,NLMPGS),PRFIML(NMOL,NLMPGS),
     R PRFPML(NMOL,NLMPGS),POTIO (NION,NLMPGS),PRFAIO(NION,NLMPGS),
     R PRFMIO(NION,NLMPGS),PRFIIO(NION,NLMPGS),PRFPIO(NION,NLMPGS),
     R POTPL (NPLS,NLMPGS),EOTAT (NATM,NLMPGS),ERFAAT(NATM,NLMPGS),
     R ERFMAT(NATM,NLMPGS),ERFIAT(NATM,NLMPGS),ERFPAT(NATM,NLMPGS),
     R EOTML (NMOL,NLMPGS),ERFAML(NMOL,NLMPGS),ERFMML(NMOL,NLMPGS),
     R ERFIML(NMOL,NLMPGS),ERFPML(NMOL,NLMPGS),EOTIO (NION,NLMPGS),
     R ERFAIO(NION,NLMPGS),ERFMIO(NION,NLMPGS),ERFIIO(NION,NLMPGS),
     R ERFPIO(NION,NLMPGS),EOTPL (NPLS,NLMPGS),
     R SPTAT (NATM,NLMPGS),SPTML (NMOL,NLMPGS),SPTIO (NION,NLMPGS),
     R SPTPL (NPLS,NLMPGS),SPTTOT(NLMPGS)     ,ADDS  (NADS,NLMPGS),
     R ALGS  (NALS,NLMPGS),SPUMP (NSPZ,NLMPGS)

      COMMON /CESTIM2/
c
c     R PDENA,PDENM,PDENI,EDENA,EDENM,EDENI,
