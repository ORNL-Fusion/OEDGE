*COMDECK CZT1
      PARAMETER (NZT1=2+NION*3+NPLS*3+NATM*3+NMOL*3+2*NRAD*NPLS+
     .                2*NREL+2*NRCX+2*NRPI)
      COMMON /CZT1/
C  NZT1, REAL
     R RSQDVI(NION),CVRSSI(NION),ALMASI(NION),
     R RSQDVP(NPLS),CVRSSP(NPLS),ALMASP(NPLS),
     R RSQDVA(NATM),CVRSSA(NATM),ALMASA(NATM),
     R RSQDVM(NMOL),CVRSSM(NMOL),ALMASM(NMOL),
     R DEFCX(NRCX),EEFCX(NRCX),
     R DEFEL(NREL),EEFEL(NREL),
     R DEFPI(NRPI),EEFPI(NRPI),
     R ZT1(NPLS,NRAD),ZRG(NPLS,NRAD),DENE,DENI
