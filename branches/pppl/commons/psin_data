c     -*-Fortran-*-
c
c     This common block is used to hold externally read in PSIN data 
c     that is used for plotting within the OUT program     
c     
      common /psindata/ surface_data,rpsimin,
     >                  r_int,z_int,psi_int,dist_int,
     >                  n_int,n_elements,external_psin,
     >                  psin_filename
      save /psindata/
c
      integer max_int
      parameter (max_int=10)
c
      real*8 surface_data(maxpts,4),rpsimin,
     >       r_int(max_int),z_int(max_int),
     >       psi_int(max_int),dist_int(max_int)
c
      integer n_int,n_elements
c
      logical external_psin 
c       
      character*200 psin_filename
c 
