c     -*-Fortran-*-
c
c     This common block contains options specifically related to
c     formatting the DIVIMP printouts  
c 
      common /printopts/ prsol21,prsol22,prsol28,coment,inner,outer
     >                  
      save /printopts/
c
      logical  prsol21,prsol22,prsol28
      character coment*100,inner*5,outer*5,
     >          sp*24,sa*2,s1*6,s2*12
c
      data sp /'                      '/
      data sa /'  '/
      data s1 /'      '/
      data s2 /'          '/
c
