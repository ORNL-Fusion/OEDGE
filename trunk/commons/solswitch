c     -*-Fortran-*-
c
c     This contains the common block with the SOL option switches
c
      integer maxopts
      parameter (maxopts=45)
c
      integer swcond,swconv,swprad,swphelp,swpei,swpcx,swmach,
     >        swvisc1,swnmom,swion,swe2d,swpow,swgperp,swmajr,
     >        swcore,swsmooth,swioni,swerror,swrecom,swionp,swpowp,
     >        swqidatiz,swqidmliz,swqidcx,swqidrec,swdetach,
     >        swgperpp,swextra,swppion,swppelec
c
      parameter (swcond=1,swconv=2,swprad=3,swphelp=4,swpei=5,
     >           swpcx=6,swmach=7,swvisc1=8,swnmom=9,swion=10,
     >           swe2d=11,swpow=12,swgperp=13,swmajr=14,
     >           swcore=15,swsmooth=16,swioni=17,swerror=18,
     >           swrecom=19,swionp=20,swpowp=21,
     >           swqidatiz=22,swqidmliz=23,swqidcx=24,swqidrec=25,
     >           swdetach=26,swgperpp=27,swextra=28,
     >           swppion=29,swppelec=30) 
c
      common /solswitch/ switch,deflist,ndef
      save /solswitch/
c
      common /activesw/ actswcond,actswconv,actswprad,actswphelp,
     >        actswpei,actswpcx,actswmach,
     >        actswvisc1,actswnmom,actswion,actswe2d,actswpow,
     >        actswgperp,actswmajr,
     >        actswcore,actswsmooth,actswioni,actswerror,
     >        actswrecom,
     >        actswqidatiz,actswqidmliz,actswqidcx,actswqidrec,
     >        actswdetach,actswppion,actswppelec
      save /activesw/
c
      real switch(maxopts),deflist(mxspts,3)     
c
      real actswcond,actswconv,actswprad,actswphelp,actswpei,
     >     actswpcx,actswmach,
     >     actswvisc1,actswnmom,actswion,actswe2d,actswpow,
     >     actswgperp,actswmajr,
     >     actswcore,actswsmooth,actswioni,actswerror,
     >     actswrecom,
     >     actswqidatiz,actswqidmliz,actswqidcx,actswqidrec,
     >     actswdetach,actswppion,actswppelec
c   
      integer ndef




